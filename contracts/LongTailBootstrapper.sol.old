abstract contract LongTailBootstrapper is SocialTokenManager {
    function setTokenManager(address managerAddr) public onlyOwner returns (address) {
        require(ISocialTokenManager(managerAddr).supportsInterface(ISocialTokenManager), "Contract must support LongTailSocialToken");

        token.setManager(managerAddr);
        return managerAddr;
    }

